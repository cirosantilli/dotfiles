# Conventions:
# - `m` prefix: multiline version
snippet a
	<a href="${1}">${2}</a>
# Anchor to Self.
snippet as
	<a href="${1}">${1}</a>
snippet c
	<code>${1}</code>
snippet d
	<div>
		${1}
	</div>
snippet h1
	<h1>${1}</h1>
snippet h2
	<h2>${1}</h2>
snippet h3
	<h3>${1}</h3>
snippet h4
	<h4>${1}</h4>
snippet h5
	<h5>${1}</h5>
snippet h6
	<h6>${1}</h6>
snippet ol
	<ol>
		<li>${1}</li>
		<li></li>
	</ol>
snippet s
	<script>
		${1}
	</script>
snippet uli
	<ul>
		<li>${1}</li>
	</ul>
